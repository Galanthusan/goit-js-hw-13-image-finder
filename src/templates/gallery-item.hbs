{{#each this}}
<div class="photo-card post"> 
        <img src="{{webformatURL}}" alt="{{tags}}" />
    <div class="stats">
        <p class="stats-item">
            <i class="material-icons">thumb_up</i><br>
            {{likes}}
        </p>
        <p class="stats-item">
            <i class="material-icons">visibility</i><br>
            {{views}}
        </p>
        <p class="stats-item">
            <i class="material-icons">comments</i><br>
            {{comments}}
        </p>
        <p class="stats-item">
            <i class="material-icons">cloud_download</i><br>
            {{downloads}}
        </p>
    </div>
</div>
{{/each}}